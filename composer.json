{
    "name": "szogyenyid/php-builder",
    "description": "A PHP trait to automagically create a Builder for any class.",
    "type": "library",
    "keywords": ["php", "trait", "builder"],
    "homepage": "https://github.com/szogyenyid/php-builder",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Szogyenyid\\PhpBuilder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "szogyenyid",
            "email": "szogyenyid@gmail.com",
            "homepage": "https://szogyenyid.dev"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "pestphp/pest": "^2.6",
        "squizlabs/php_codesniffer": "^3.7",
        "phpcompatibility/php-compatibility": "^9.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
